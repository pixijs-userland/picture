{
  "name": "pixi-picture",
  "version": "2.0.4",
  "description": "PixiJS v7 backport to v5, advanced filter renderer, implements advanced blending modes",
  "author": "Ivan Popelyshev",
  "contributors": [
    "Ivan Popelyshev <ivan.popelyshev@gmail.com>"
  ],
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "extensionConfig": {
    "lint": ["src"],
    "namespace": "PIXI.picture",
    "docsName": "PixiJS Picture",
    "docsCopyright": "Copyright &copy; 2015 - 2023 Ivan Popelyshev",
    "docsTitle": "PixiJS Picture blend API Documentation",
    "docsDescription": "Documentation for PixiJS Picture library",
    "docsKeyword": "docs, documentation, pixi, pixijs, rendering, pixi-picture, shader, javascript, jsdoc",
    "globals": {
      "pixi.js": "PIXI"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "http://www.pixijs.com/",
  "bugs": "https://github.com/pixijs/picture/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixijs/picture.git"
  },
  "scripts": {
    "clean": "xs clean",
    "start": "xs serve",
    "watch": "xs watch",
    "build": "xs build",
    "lint": "xs lint",
    "lint:fix": "xs lint --fix",
    "types": "xs types",
    "release": "xs release",
    "docs": "xs docs",
    "deploy": "xs deploy",
    "test": "xs build,docs"
  },
  "files": [
    "dist/",
    "lib/",
    "global.d.ts"
  ],
  "peerDependencies": {
    "pixi.js": "~5.3.9"
  },
  "devDependencies": {
    "pixi.js": "~5.3.9",
    "@pixi/extension-scripts": "^1.8.1"
  }
}
